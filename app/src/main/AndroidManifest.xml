<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.squid" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/fichier_12x_3"
        android:label="@string/app_name"
        android:roundIcon="@drawable/fichier_12x_3"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar" >
        <activity
            android:name="com.example.squid1.Splash"
            android:exported="true" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.example.squid1.Login.RegisterActivity" />
        <activity android:name="com.example.squid1.Login.LoginActivity" />
        <activity android:name="com.example.squid1.Cart.CheckoutActivity" />
        <activity android:name="com.example.squid1.Search.ResultSearchActivity" />
        <activity
            android:name="com.example.squid1.MainActivity"
            android:exported="true" />

    </application>

</manifest>